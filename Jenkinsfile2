node('ubuntu') 
{
    stages
    {
        stage('clone')
        {
            git branch: 'JS', 
                credentialsId: 'UBUNTU', 
                url: 'https://github.com/tarunkumarpendem/js-e2e-express-server.git'
        }
        stage('build and install')
        {
            script 
            {
                 'npm install run build run test'
            }
            
        }
    }
}    
